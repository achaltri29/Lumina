<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <radialGradient id="outerGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#1a1a1a;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="middleGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#404040;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#2d2d2d;stop-opacity:1" />
    </radialGradient>
    <radialGradient id="innerGradient" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#666666;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#4a4a4a;stop-opacity:1" />
    </radialGradient>
  </defs>
  
  <!-- Outer circle -->
  <circle cx="16" cy="16" r="15" fill="url(#outerGradient)" stroke="#000000" stroke-width="1"/>
  
  <!-- Middle circle -->
  <circle cx="16" cy="16" r="10" fill="url(#middleGradient)"/>
  
  <!-- Inner circle -->
  <circle cx="16" cy="16" r="6" fill="url(#innerGradient)"/>
  
  <!-- Center highlight -->
  <circle cx="16" cy="16" r="2" fill="#888888" opacity="0.8"/>
